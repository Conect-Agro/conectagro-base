    "echo 'Planned takeover complete on {failureCluster}. Master: {failedHost}:{failedPort}, promoted: {successorHost}:{successorPort}' >> /tmp/recovery.log"
  ],
  "PostFailoverProcesses": [
    "echo '(for all types) Failover complete on {failureCluster}. Failed: {failedHost}:{failedPort}, promoted: {successorHost}:{successorPort}' >> /tmp/recovery.log"
